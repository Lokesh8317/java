class Node
{
  int data;
  Node next;
}
public class Main
{
  public static void main(String[] args)
  {
    Node a=new Node();
    a.data=100;
    Node b=new Node();
    b.data=200;
    Node c=new Node();
    c.data=300;
    a.next=b;
    b.next=c;
    System.out.println(a.data);
    System.out.println(b.data);
    System.out.println(c.data);
    System.out.println(a.next);
    System.out.println(b.next);
    System.out.println(c.next);
  }
}
