package com.learn.basics;
import java.util.*;
public class arrays {

	public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
    int n=s.nextInt();
    int i;
    int add=0,sub=0,mul=1;
    int arr[]=new int[n];
    for(i=0;i<n;i++)
    {
    	arr[i]=s.nextInt();
    }
    for(i=0;i<n;i++)
    {
    	add+=arr[i];
    	mul=mul*arr[i];
    	sub=mul-add;
    }
    System.out.println(add);
    System.out.println(mul);
    System.out.println(sub);
	}

}
