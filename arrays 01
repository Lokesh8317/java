package com.learn.basics;
import java.util.*;
public class arrays {

	public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
	System.out.println("size of the array:");
    int n=s.nextInt();
    int i;
    System.out.println("elements of the array:");
    int arr[]=new int[n];
    for(i=0;i<n;i++)
    {
    	arr[i]=s.nextInt();
    }
    for(i=0;i<n;i++)
    {
    	System.out.print(arr[i]*10+" ");
    }
	}

}
