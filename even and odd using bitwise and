package com.learn.basics;
import java.util.*;
public class even_odd {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		if((n&1)==0)
		{
			System.out.println("the number is even");
		}
		else 
		{
			System.out.println("the number is odd");
		}

	}

}
